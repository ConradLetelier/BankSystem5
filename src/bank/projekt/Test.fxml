<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bank.projekt.MainpageController">
   <top>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <MenuBar>
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Close" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Delete" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
            <HBox spacing="10.0">
               <children>
                  <Label text="Name" />
                  <TextField />
                  <Label text="Person number" />
                  <TextField />
                  <Region HBox.hgrow="ALWAYS" />
                  <Button mnemonicParsing="false" text="Search" />
               </children>
               <VBox.margin>
                  <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
               </VBox.margin>
            </HBox>
         </children>
         <BorderPane.margin>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </BorderPane.margin>
      </VBox>
   </top>
   <right>
      <VBox alignment="CENTER" prefHeight="405.0" prefWidth="163.0" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <Region VBox.vgrow="ALWAYS" />
            <Label text="Customer options">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <Button fx:id="addcustomer" mnemonicParsing="false" onAction="#addCustomer" prefWidth="160.0" text="Add customer" />
            <Button fx:id="removecustomer" mnemonicParsing="false" onAction="#removeCustomer" prefWidth="160.0" text="Remove customer" />
            <Button fx:id="changecustomer" mnemonicParsing="false" onAction="#changeCustomer" prefWidth="160.0" text="Change customer info" />
            <Label text="Account options">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Label>
            <Separator prefWidth="200.0" />
            <Button mnemonicParsing="false" prefWidth="160.0" text="Add new savings account" />
            <Button mnemonicParsing="false" prefWidth="160.0" text="Add new credit account" />
            <Button mnemonicParsing="false" prefWidth="160.0" text="Remove account" />
            <Button mnemonicParsing="false" prefWidth="160.0" text="Withdraw" />
            <Button mnemonicParsing="false" prefWidth="160.0" text="Deposit" />
            <Button mnemonicParsing="false" prefWidth="160.0" text="Transaction History" />
            <Region prefHeight="0.0" prefWidth="163.0" VBox.vgrow="ALWAYS" />
            <Separator prefHeight="0.0" prefWidth="163.0" />
         </children>
         <BorderPane.margin>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </BorderPane.margin>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </VBox>
   </right>
   <bottom>
      <HBox BorderPane.alignment="CENTER">
         <children>
            <Label text="Label" />
         </children>
         <BorderPane.margin>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </BorderPane.margin>
         <padding>
            <Insets left="5.0" />
         </padding>
      </HBox>
   </bottom>
   <left>
      <VBox alignment="CENTER_LEFT" spacing="2.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Customers">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets left="60.0" />
               </padding>
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
            <TableView prefHeight="405.0" prefWidth="249.0" VBox.vgrow="ALWAYS">
              <columns>
                <TableColumn prefWidth="110.0" text="Name" />
                <TableColumn prefWidth="137.0" text="Person Number" />
              </columns>
            </TableView>
         </children>
         <opaqueInsets>
            <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
         </opaqueInsets>
         <BorderPane.margin>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </BorderPane.margin>
      </VBox>
   </left>
   <center>
      <AnchorPane prefHeight="450.0" prefWidth="411.0" BorderPane.alignment="CENTER">
         <children>
            <VBox alignment="CENTER" spacing="2.0" AnchorPane.bottomAnchor="3.0" AnchorPane.leftAnchor="3.0" AnchorPane.rightAnchor="3.0" AnchorPane.topAnchor="3.0">
               <children>
                  <Label alignment="CENTER" contentDisplay="TOP" prefHeight="30.0" prefWidth="87.0" text="Accounts">
                     <font>
                        <Font name="System Bold" size="20.0" />
                     </font>
                  </Label>
                  <TableView prefHeight="469.0" prefWidth="405.0" VBox.vgrow="ALWAYS">
                     <columns>
                        <TableColumn prefWidth="175.0" text="Type of account" />
                        <TableColumn prefWidth="166.0" text="Account ID" />
                        <TableColumn prefWidth="131.0" text="Balance" />
                     </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
                  <Region VBox.vgrow="ALWAYS" />
               </children>
               <opaqueInsets>
                  <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
               </opaqueInsets>
            </VBox>
         </children>
      </AnchorPane>
   </center>
</BorderPane>
